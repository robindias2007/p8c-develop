        <div class="header">
        <div id="nav-header">
           <ul>
               <a href="/"><li id="logo"><%= image_tag 'logo.png' %>THE LENSE</li></a>
               
               <% if user_signed_in? %>
               <a href="/forms/new"><li id="curate">Curate<i class="material-icons" , id="nav-icons">add_circle_outline</i></li></a>
               <a class="dropdown-toggle"  data-toggle="dropdown"><i class="material-icons" style="font-size:30px; float: right; ">account_circle</i></a>
                <ul class="dropdown-menu">
               <li><a href="/forms">View Your Boards</a></li><br>
              <li><%= link_to('Logout', destroy_user_session_path, :method => :delete) %>        </li>
               </ul>




               <% else %>
