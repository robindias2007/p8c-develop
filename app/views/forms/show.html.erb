               <a href="/users/sign_in"><li id="user">Sign In/Sign Up<i class="material-icons" , id="nav-icons">account_circle</i></li></a>
               <% end %>

                <li id="search-box">
                    <form>
                        <input type="search", id="sebar" placeholder="Search...">
                        <a class="button"><i class="material-icons", id="nav-icons">search</i></a>
                    </form> 
               </li>
            </ul>
        </div>
        </div>

              
        
        
      <div id="nav-menu">
        <ul id="subh">
            <li>Trending</li>
