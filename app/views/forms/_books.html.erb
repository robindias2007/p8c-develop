<% if form.bookmark == true %>
  <%= link_to "", booknot_form_path(form.id), remote: true,
                        id: "book_#{form.id}",
                        class:"glyphicon glyphicon-bookmark" %>
<% else %>
  <%= link_to "", book_form_path(form.id), remote: true,
                        id: "book_#{form.id}",
                        class:"glyphicon glyphicon-bookmark empty" %>
<% end %>



<style type="text/css">
  .empty
  { 
  	color: white;
  	text-shadow: -2px 0 #9c1717, 0 2px #9c1717, 2px 0 #9c1717, 0 -2px #9c1717;
  }
</style>